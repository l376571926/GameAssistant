<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qianfeng.gameassistant"
    android:versionCode="1"
    android:versionName="v1.2.4" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name="com.qianfeng.gameassistant.other.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.qianfeng.gameassistant.other.ui.HomeActivity" />
        <activity
            android:name="com.qianfeng.gameassistant.other.ui.GuideActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen" />
        <activity android:name="com.qianfeng.gameassistant.module2.ui.GiftInfoActivity" />
        <activity android:name="com.qianfeng.gameassistant.other.ui.LoginActivity" />
        <activity android:name="com.qianfeng.gameassistant.module4.ui.PrizeDetailActivity" />
        <activity android:name="com.qianfeng.gameassistant.module1.ui.GameDetailActivity" />
        <activity android:name="com.qianfeng.gameassistant.module5.ui.MiniGameActivity" >
        </activity>
    </application>

</manifest>